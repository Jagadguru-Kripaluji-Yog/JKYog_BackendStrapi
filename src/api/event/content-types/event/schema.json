{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "event_categories": {
      "type": "enumeration",
      "enum": [
        "Programs",
        "Retreat",
        "Workshop"
      ]
    },
    "event_type": {
      "type": "enumeration",
      "enum": [
        "Online",
        "None",
        "Sanskriti"
      ]
    },
    "banner_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "list_page_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "short_Description": {
      "type": "text"
    },
    "Body": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "timezone": {
      "type": "enumeration",
      "enum": [
        "HAST",
        "AKST",
        "PST",
        "MST",
        "CST",
        "EST",
        "IST",
        "NPT",
        "UAE",
        "SGT"
      ]
    },
    "event_start_date_and_time": {
      "type": "datetime"
    },
    "event_end_date_and_time": {
      "type": "datetime"
    },
    "event_location_details": {
      "type": "string"
    },
    "event_organizer_detail": {
      "type": "string"
    },
    "coordinator_name": {
      "type": "string"
    },
    "learn_more_url": {
      "type": "string"
    },
    "banner_image_link": {
      "type": "string"
    },
    "banner_image_target": {
      "type": "enumeration",
      "enum": [
        "New Tab"
      ]
    },
    "featured_image_alternative_text": {
      "type": "string"
    },
    "list_page_image_alternative_text": {
      "type": "string"
    },
    "recurrence": {
      "type": "enumeration",
      "enum": [
        "None",
        "Daily",
        "Weekly",
        "Monthly",
        "Yearly",
        "Custom"
      ]
    },
    "recurring_event_time": {
      "type": "string"
    },
    "event_cost": {
      "type": "string"
    },
    "event_tags": {
      "type": "string"
    },
    "cost_description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "event_video_youtube_url": {
      "type": "string"
    },
    "events_caraousel": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
